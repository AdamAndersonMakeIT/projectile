GET         /                                        controllers.HomeController.index
GET         /file/view/*path                         controllers.HomeController.viewFile(path)
POST        /file/edit/*path                         controllers.HomeController.editFile(path)
GET         /testbed                                 controllers.HomeController.testbed


# Inputs
GET         /input/new                               controllers.InputController.formNew()
POST        /input/save                              controllers.InputController.save()

GET         /input/all/refresh                       controllers.InputController.refreshAll

GET         /input/:key                              controllers.InputController.detail(key)
GET         /input/:key/remove                       controllers.InputController.remove(key)
GET         /input/:key/form                         controllers.InputController.form(key)
GET         /input/:key/refresh                      controllers.InputController.refresh(key)


# Projects
GET         /project                                 controllers.ProjectController.formNew()
POST        /project                                 controllers.ProjectController.save()

GET         /project/all/audit                       controllers.BulkProjectController.auditAll
GET         /project/all/export                      controllers.BulkProjectController.exportAll

GET         /project/:key                            controllers.ProjectController.detail(key)
GET         /project/:key/remove                     controllers.ProjectController.remove(key)
GET         /project/:key/form                       controllers.ProjectController.form(key)
GET         /project/:key/export                     controllers.ProjectController.export(key)
GET         /project/:key/audit                      controllers.ProjectController.audit(key)

GET         /project/:key/enum                       controllers.ProjectEnumController.formNew(key)
POST        /project/:key/enum                       controllers.ProjectEnumController.save(key)
GET         /project/:key/enum/:e                    controllers.ProjectEnumController.detail(key, e)
GET         /project/:key/enum/:e/form               controllers.ProjectEnumController.form(key, e)
GET         /project/:key/enum/:e/remove             controllers.ProjectEnumController.remove(key, e)

GET         /project/:key/model                      controllers.ProjectModelController.formNew(key)
POST        /project/:key/model                      controllers.ProjectModelController.save(key)
GET         /project/:key/model/:m                   controllers.ProjectModelController.detail(key, m)
GET         /project/:key/model/:m/edit              controllers.ProjectModelController.form(key, m)
GET         /project/:key/model/:m/remove            controllers.ProjectModelController.remove(key, m)

GET         /project/:key/service                    controllers.ProjectServiceController.formNew(key)
POST        /project/:key/service                    controllers.ProjectServiceController.save(key)
GET         /project/:key/service/:s                 controllers.ProjectServiceController.detail(key, s)
GET         /project/:key/service/:s/edit            controllers.ProjectServiceController.form(key, s)
GET         /project/:key/service/:s/remove          controllers.ProjectServiceController.remove(key, s)


